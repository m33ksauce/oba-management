{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint-fix": "npm run lint -- --fix",
    "build": "tsc",
    "build:watch": "tsc -w",
    "serve": "npm run build && firebase emulators:start --only firestore,functions,storage",
    "serve:watch": "npm run build && firebase emulators:start --only firestore,functions,storage",
    "shell": "npm run build && npm run start-store && firebase functions:shell",
    "start-store": "firebase emulators:start --only firestore",
    "start": "npm run build && node lib/index.js",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.178.0",
    "@aws-sdk/client-s3": "^3.171.0",
    "aws-sdk": "^2.1216.0",
    "body-parser": "^1.20.0",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.17.3",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.21.2",
    "morgan": "^1.10.0",
    "stream-to-array": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.18",
    "@types/stream-to-array": "^2.3.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^4.5.4"
  },
  "private": true
}
